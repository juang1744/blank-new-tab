{
  "name": "Blank New Tab",
  "description": "Replace chrome://newtab with a blank page",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "action": {
    "default_icon": {
      "16": "images/extension_icon16.png",
      "32": "images/extension_icon32.png",
      "48": "images/extension_icon48.png",
      "128": "images/extension_icon128.png"
    }
  },
  "icons": {
    "16": "images/extension_icon16.png",
    "32": "images/extension_icon32.png",
    "48": "images/extension_icon48.png",
    "128": "images/extension_icon128.png"
  }
}